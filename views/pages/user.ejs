<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Sigin-Javaloca</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/css/bootstrap.min.css" integrity="sha384-B0vP5xmATw1+K9KRQjQERJvTumQW0nPEzvF6L/Z6nronJ3oUOFUFpCjEUQouq2+l" crossorigin="anonymous">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons"
          rel="stylesheet">
          <link rel="stylesheet" href="/css/tamplate_1.css">
          <link href="https://fonts.googleapis.com/icon?family=Material+Icons"
          rel="stylesheet">
</head>

<body>
  <header>
      <div class="container-fluid p-0">
      <%- include('../layouts/navbar.ejs'); %>
      </div>
  </header>
      <br>

      <section class="section-1"> 
        <div class="container text-center">
            <div class="row">
                <div class="col-md-6">
                    <div class="pray">
                        <img src="/img/pantai.jpg" alt="pantai" >
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="panel text-left">
                        <h1>Sign in</h1>
                        <form method="post" action="/account/userupdate">
                            <% datauser.forEach((User) => { %>
                            <div class="form-group">
                              <label for="inputname">Name</label>
                              <input type="text" value="<%= User.name %>" class="form-control" name="password_signin" id="inputPassword" placeholder="password">
                            </div>
                            <div class="form-group">
                                <label for="inputname">Address</label>
                                <input type="text" class="form-control" value="vv" name="password_signin" id="inputPassword" placeholder="password">
                              </div> 
                              <div class="form-group">
                                <label for="inputname">New Password</label>
                                <input type="text" class="form-control" name="password_signin" id="inputPassword" placeholder="password">
                              </div>
                              <div class="form-group">
                                <label for="inputname">Password confirmation</label>
                                <input type="text" class="form-control" name="password_signin" id="inputPassword" placeholder="password">
                              </div>      
                            <button type="submit" class="btn btn-primary">Sign up</button>
                        </form>
                        <% }) %>
                    </div>
                </div>
            </div>
        </div>
        
        </section>

        <%- include('../layouts/footer.ejs'); %>
      

        
              <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
</body>
</html>